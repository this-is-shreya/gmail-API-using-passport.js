<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
    <link
      rel="stylesheet"
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/css/bootstrap.min.css"
    />
    <meta http-equiv="refresh" content="60" />
  </head>
  <body>
    <h1>Your email details:</h1>
    <table class="table">
      <thead>
        <tr>
          <th scope="col">#</th>

          <th scope="col">Snippet</th>
          <th scope="col">Thread Id</th>
        </tr>
      </thead>
      <tbody>
        <%let i=1;%> <%arr.forEach(item=>{%>
        <tr>
          <th scope="row"><%= i %></th>
          <td><%= item.snippet%></td>
          <td><%= item.id%></td>
          <% i++; %>
        </tr>
        <%})%>
      </tbody>
    </table>
  </body>
</html>
